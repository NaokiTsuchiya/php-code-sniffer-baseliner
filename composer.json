{
    "name": "isaac/php-code-sniffer-baseliner",
    "description": "ISAAC PHP Code Sniffer Baseliner",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": "~7.3.0 || ~7.4.0 || ~8.0.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "ISAAC\\CodeSnifferBaseliner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ISAAC\\CodeSnifferBaseliner\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/phpcs-baseliner"
    ],
    "require-dev": {
        "isaac/composer-git-hooks-standard": "^1.2",
        "isaac/php-code-sniffer-standard": "^19.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.65",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "isaac": {
            "type": "composer",
            "url": "https://composer-packages.hq.isaac.nl/repository"
        }
    }
}
